scwin.sbm_updateClaim_submitdone = function (e) {
    location.href = "/InsWebApp/websquare/websquare.html?w2xPath=/InsWebApp/ui/audit/insure-ocr.xml";
    $p.closePopup("img_popup");
};

function openPopup(link, popupId, popupName, size = {}) {
    requires("uiplugin.popup"); 
	var winWid = $(window).width();
	var winHei = $(window).height();
	var popWid = size.width ? size.width : 1000;
	var popHei = size.height ? size.height : 600;
	var sumLeft = (winWid - popWid) / 2;
	var sumTop = (winHei - popHei) / 2;
					
	var opts = {
					"id" : popupId,
					"type" : "litewindow",
					"width" : popWid + "px",
					"height" : popHei + "px",
					"top" : sumTop, 
					"left" : sumLeft,
					"popupName" : popupName,
					"modal" : true, 
					"useIFrame" : false,
					"title" : "focusTest",
					"useATagBtn" : false,
					"frameMode" : "wframe",
				};

	$p.openPopup(link, opts);
}

scwin.grd_befClaim_oncellclick = function (rowIndex, columnIndex, columnId) {
    let claimNo = dl_claimbefore.getRowJSON(rowIndex).claim_no;
