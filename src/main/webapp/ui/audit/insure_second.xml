        <script lazy="false" type="text/javascript"><![CDATA[
scwin.onpageload = function() {
    scwin.index = 0;
    scwin.genList = [];

    scwin.emp = JSON.parse(localStorage.getItem("employee"));
    // todo: role에 따라 변경.. empName
    
    scwin.claim = JSON.parse(localStorage.getItem("claim"));
    dm_header.set("claimNo", scwin.claim.claimNo);
    dm_header.set("patient", scwin.claim.userName);

    dm_claim.set("claim_no", scwin.claim.claimNo);
    let reqData = {"elData":{"ID": scwin.claim.ID }};
    $c.sbm.execute(sbm_befClaim, reqData);
    $c.sbm.execute(sbm_getSubInsu, reqData);

    let request = {"claimVo": {"claim_no": scwin.claim.claimNo}};
    $c.sbm.execute(sbm_getClaim, request);

    var jsnOptions = {
        id: "img_popup",
        type: "browserPopup", //필수 고정
        width: "1000px",
        height: "800px",
        popupName: "청구 영수증"
    };

    $p.openPopup("/InsWebApp/ui/audit/img_modal.xml", jsnOptions);

    scwin.genList.push(scwin.index);
	gen_date.insertChild(scwin.index++);   

};

let isDateDrop = false;

scwin.sbm_getClaim_submitdone = function (e) {
    // 대기, 결재중 아니면 그냥 넘어가게
    let status = dm_claim.getJSON().status;
    if (status !== "대기" && status !== "결재중") {
        isDateDrop = true;
    }
    
};
        
scwin.btn_next_onclick = function (e) {
    let missingFields = [];
    let isClaimOffset = false;
    let isHcode = false;
    let isSaveOffset = false;

    // 상계 
    const claimNo = ipt_claimNo.getValue();
    const reason = ipt_reason.getValue();
    const charge = ipt_charge.getValue();

    if (claimNo !== "" && reason !== "" && charge !== "") {
        // 상계 저장 처리
        dm_claimoffset.set("claim_no", localStorage.getItem("claimNo")); 
        isClaimOffset = true;
        isSaveOffset = true;
    } 
    else if (claimNo === "" && reason === "" && charge === "") { // (상계) 모든 값이 비어있는 경우
        isClaimOffset = true;
    } 
    else {
        missingFields.push("상계 금액 정보");
    }

    // 입통원 일자
    let saveList = [];
    for (let i = 0; i < scwin.genList.length; i++) {
        const idx = scwin.genList[i];
        let date = gen_date.getChild(idx, "ipt_startDate").realValue;
        let date2 = gen_date.getChild(idx, "ipt_endDate").realValue;
        let imgIdx = gen_date.getChild(idx, "ipt_idx").realValue;
            
        if (!date || !imgIdx) {
            missingFields.push("입통원일자 목록");
            break;
        }

        let dateRange = date2 ? `${date}~${date2}` : date;

        saveList.push({
            dateRange: dateRange,
            imgIdx: imgIdx
        });
        if (i === scwin.genList.length -1) {
            isDateDrop = true;
        }
    }

    localStorage.setItem('dateList', JSON.stringify(saveList));

    let hcode1 = ipt_hcode1.getValue();
    let hcode2 = ipt_hcode2.getValue();
    let hcode2Date = ipt_hcodeDate.getValue();

    if (hcode1 !== "" && hcode2Date !== "") {
        isHcode = true;
        dm_claim.set("disease_code", hcode1);
        dm_claim.set("date_of_accident", hcode2Date);
    } else {
        missingFields.push("청구내역 및 처리정보 (병명코드, 재해 일자는 필수 입력입니다.)");
    }
    
    if (hcode2 !== "" ) {
        dm_claim.set("disaster_code", hcode2);  
    } 

    if (isClaimOffset && isDateDrop && isHcode) {
        // 서브미션 호출
        if (isSaveOffset) {
            $c.sbm.execute(sbm_insertOffset); 
        }
        $c.sbm.execute(sbm_updateClaim);
    }
    else {
        alert("다음 항목의 입력을 확인하세요\n" + missingFields.join(", "));
    }
};

scwin.btn_befo_onclick = function (e) {
    location.href = "/InsWebApp/websquare/websquare.html?w2xPath=/InsWebApp/ui/audit/insure_first.xml";
    //parent.WebSquare.util.getComponentById("web_container").setSrc("/InsWebApp/ui/audit/insure_first.xml");
};

scwin.btn_add_date_onclick = function (e) {
    scwin.genList.push(scwin.index);
    gen_date.insertChild(scwin.index++);
};

scwin.btn_del_date_onclick = function (e) {
    let dateGen = this.getGenerator();
    let numIndex = this.getGeneratedIndex();

    dateGen.removeChild(numIndex);

    const indexToRemove = scwin.genList.indexOf(numIndex); 
    if (indexToRemove !== -1) {
        scwin.genList.splice(indexToRemove, 1); 
    }
};

