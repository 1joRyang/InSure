				ev:submiterror="" abortTrigger="">
			</xf:submission>
		</xf:model>
		<w2:layoutInfo />
		<w2:publicInfo method="" />
		<script lazy="false" type="text/javascript"><![CDATA[
scwin.onpageload = async function () {
    console.log("온페이지 로드")

    await $c.sbm.execute($p, sbm_selectEmployeeVoList);
};

scwin.sbm_selectEmployeeVoList_submitdone = function (e) {
    console.log("submission done");;
};]]></script>
