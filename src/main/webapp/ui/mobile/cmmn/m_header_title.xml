<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
    xmlns:ev="http://www.w3.org/2001/xml-events"
    xmlns:w2="http://www.inswave.com/websquare" xmlns:xf="http://www.w3.org/2002/xforms">
    <head>
    	<w2:type>COMPONENT</w2:type>
    	<w2:buildDate />
    	<w2:MSA />
    	<xf:model>
    		<w2:dataCollection baseNode="map" />
    		<w2:workflowCollection />
    	</xf:model>
    	<w2:layoutInfo />
    	<w2:publicInfo method="" />
    	<script lazy="false" type="text/javascript"><![CDATA[
scwin.onpageload = function() {
	const path = window.location.href;

    let title = "홈";
    if (path.includes("m_home.xml")) {
        svg_before.hide();
    } else if (path.includes("my-claims-m.xml")) {
        title = "청구내역 조회";
    } else if (path.includes("claim-detail.xml")) {
        title = "청구세부 조회"
    } else if (path.includes("first.xml")) {
        title = "나의 보험";
    } else if (path.includes("suppDocs.xml")) {
        title = "청구보완 요청";
    } else if (path.includes("insurePageTab1.xml")) {
        title = "나의 보험";
    }else if(path.includes("requestDoc.xml")){
        title="제출 서류 안내";
    }else if(path.includes("m_terms.xml")){
        title="약관 안내";
    }

    m_htitle.setValue(title); 
};

scwin.svg_before_onclick = function () {
    window.history.back();
};
]]></script>
    	<w2:require as="udc_notification" type="page" variableClone="true" src="/InsWebApp/cm/udc/matrix/notification.xml"></w2:require>
    </head>
    <body ev:onpageload="scwin.onpageload">
    	<xf:group class="sub_contents" id="" meta_snippetCategory="00_화면시작" meta_snippetKeyComponent="true" meta_snippetName="0_01 페이지시작"
    		style="background-color: white;padding: 15px;box-shadow: 0 2px 8px rgba(0,0,0,.1);">
    		<xf:group class="" id="" meta_snippetCategory="02_타이틀" meta_snippetKeyComponent="true" meta_snippetName="2_01 페이지타이틀"
    			style="margin: 0;justify-content: space-between;display: flex;">
    			<xf:group style="display: flex;" id="">
    				<w2:anchor outerDiv="false" style="height: 100%;width: 28px;margin-top: 3px; background-size: 24px 24px !important;" id="svg_before" class="prev_icon" ev:onclick="scwin.svg_before_onclick">
    					<xf:label></xf:label>
    				</w2:anchor>
    				<xf:group style="display: flex;" id="">
    					<w2:textbox style="font-size: 18px;font-weight: bold;text-align: center;margin-top: 8px;" id="m_htitle" label="보험금 청구"></w2:textbox>
    				</xf:group>
    			</xf:group>
    			<xf:group class="" id="" style="display: flex;gap: 10px;">
    				<w2:udc_notification message="" style="margin-top: 5px;" title=""></w2:udc_notification>
    			</xf:group>
    		</xf:group>
    	</xf:group>
    </body>
</html>
