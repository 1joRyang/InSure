		<script lazy="false" type="text/javascript"><![CDATA[
	scwin.onpageload = function() {		
		//search_box
        var cptIbxName = $p.getComponentById("searchComp_1");
        if(cptIbxName == undefined){
			search_box.hide();
        }
		
		// 결재요청 조회
		scwin.btn_search_onclick();
	};

	/************************** 이벤트 함수 *************************/
	
	/**
	 * 조회 버튼 클릭 이벤트
	 * @date
	 * @memberOf
	 * @param 
	 * @returns
	 * @author 
	 * @example
	 */		   
	scwin.btn_search_onclick = function(e) {
	
		// PGAE번호 초기화
		dmp_approvalReqVo.set("pageIndex","1");
		pgl_approvalReqList.setSelectedIndex(1);

		// 서버통신 - 결재요청 조회
		$c.sbm.execute($p, sbm_selectApprovalReqVoList);
	};
	
	scwin.sbm_approvalReqList_submitdone = function(e) {

		// error 수신시
		var elData = e.responseJSON.elData;
		var elHeader  = e.responseJSON.elHeader;
		if(elHeader == null || elHeader == "" || elHeader == "undefiend" || elHeader.resSuc == false) {
			$c.win.alert(`에러코드 : ${elHeader.resCode}\n에러메시지 : ${elHeader.resMsg}`);
			return false;
		}

		// 결재요청 dataMap 초기화
		dmp_approvalReqVoDetail.setEmptyValue();

		// 총 레코드 수 업데이트
        spn_listCnt.setLabel(elData.totalCount);
        
        // 데이터를 그리드에 셋팅
        dlt_approvalReqVoList.setJSON(elData.approvalReqVoList);
        
        // 페이지 네비게이터 업데이트
        var totalPageCount = Math.ceil(elData.totalCount / dmp_approvalReqVo.get("pageSize"));
        pgl_approvalReqList.setCount(totalPageCount);
        
		// 페이징 정보 업데이트
        spn_totalPageCount.setValue(totalPageCount);
        spn_pageIndex.setValue(dmp_approvalReqVo.get("pageIndex"));
        pgl_approvalReqList.setSelectedIndex(dmp_approvalReqVo.get("pageIndex"));
	};		

	/**
	 * 페이지 번호 클릭 이벤트
	 * @date
	 * @memberOf
	 * @param 
	 * @returns 
	 * @author 
	 * @example
	 */
	scwin.pgl_approvalReqList_onclick = function(idx) {
	
		// 페이지 번호 세팅
		dmp_approvalReqVo.set( "pageIndex" , idx ); 
		// 서버통신 - 사원목록 조회
		$c.sbm.execute($p, sbm_selectApprovalReqVoList);
	};

]]></script>
