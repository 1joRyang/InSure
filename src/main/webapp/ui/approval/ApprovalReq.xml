	 * @date
	 * @memberOf
	 * @param 
	 * @returns
	 * @author 
	 * @example
	 */	
	scwin.btn_save_onclick = function(e) {
		
		// 유효성 체크
		var valInfo = [
			{ id: "approval_id", mandatory: true }
           
		];
		
		if ($c.data.validateGroup(grp_approvalReqInfo, valInfo) === true) {
			var promise = $c.win.confirm("저장하시겠습니까?");
			promise.then(function(result) {  
				if (result){
					$c.sbm.execute(sbm_insertApprovalReqVo);
				}
			})
		} 	
	};

