        <script lazy="false" type="text/javascript"><![CDATA[
scwin.onpageload = function() {
    scwin.emp = JSON.parse(localStorage.getItem("employee"));
    dm_empInfo.set("emp_no", scwin.emp.empNo);
    dm_empInfo.set("emp_name", scwin.emp.empName)

    // 가져오기
    dm_wait.set("emp_no", scwin.emp.empNo);
    $c.sbm.execute(sbm_getWaitList);

    dm_done.set("emp_no", scwin.emp.empNo);
    $c.sbm.execute(sbm_getDoneList);

    dm_approval.set("emp_no", scwin.emp.empNo);
    $c.sbm.execute(sbm_getApprovalList);

    dm_return.set("emp_no", scwin.emp.empNo);
    $c.sbm.execute(sbm_getReturnList);
};     

function moveNsave(rowData) {
    // claimNo, ID, user_name, status, empName
    localStorage.removeItem("claimImg");
    
    const claim = {
        claimNo: rowData.claim_no,
        ID: rowData.ID,
        empName: rowData.emp_name,
        userName: rowData.user_name,
        empNo:rowData.emp_no,
        status: rowData.status
    };
    localStorage.setItem("claim", JSON.stringify(claim));

    if (rowData.status === "결재완료" || rowData.status == "결재반려") {
        location.href = "/InsWebApp/websquare/websquare.html?w2xPath=/InsWebApp/ui/audit/insure-result.xml";
    } else {
        location.href = "/InsWebApp/websquare/websquare.html?w2xPath=/InsWebApp/ui/audit/insure_first.xml";
    }
    
    
}

scwin.grid_w_oncelldblclick = function (rowIndex, columnIndex, columnId) {
    let rowData = grid_w.getRowJSON(rowIndex);
    moveNsave(rowData);
};

scwin.grid_d_oncelldblclick = function (rowIndex, columnIndex, columnId) {
    let rowData = grid_d.getRowJSON(rowIndex);
    moveNsave(rowData);
};

scwin.grid_ap_oncelldblclick = function (rowIndex, columnIndex, columnId) {
    let rowData = grid_ap.getRowJSON(rowIndex);
    moveNsave(rowData);
};

scwin.grd_re_oncelldblclick = function (rowIndex, columnIndex, columnId) {
    let rowData = grd_re.getRowJSON(rowIndex);
    moveNsave(rowData);
};
]]></script>
